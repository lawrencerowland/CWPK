<owl:AnnotationProperty rdf:about="#representations">
  <core:prefLabel xml:lang="en">representations</core:prefLabel>
  <core:definition xml:lang="en">Pointers or indicators, including symbolic ones such as text or URLs, that draw attention to the actual or dynamic object.</core:definition>
  <core:altLabel xml:lang="en">annotations</core:altLabel>
  <core:altLabel xml:lang="en">indexicals</core:altLabel>
  <core:altLabel xml:lang="en">metadata</core:altLabel>
  <core:scopeNote xml:lang="en">A Thirdness</core:scopeNote>
  <core:scopeNote xml:lang="en">Indexicals are of the form of re:A; that is, statements about or references to subject A.</core:scopeNote>
  <core:scopeNote xml:lang="en">Indexicals can not be inferred over because there are not class-subclass relations, nor natural relatedness, but may be analyzed in other ways.</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#codes">
  <rdfs:subPropertyOf rdf:resource="#indexes"/>
  <core:prefLabel xml:lang="en">codes</core:prefLabel>
  <core:definition xml:lang="en">An assignment of an object to a symbolic token or string that acts to group that object with similar items; the generation and interpretation of the token is (often) done in relation to an understood scheme or system.</core:definition>
  <core:scopeNote xml:lang="en">A Thirdness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#indexes">
  <rdfs:subPropertyOf rdf:resource="#representations"/>
  <core:prefLabel xml:lang="en">indexes</core:prefLabel>
  <core:definition xml:lang="en">These ae indirect references or pointers that help situate or draw attention to the subject.</core:definition>
  <core:scopeNote xml:lang="en">A Secondness</core:scopeNote>
  <core:scopeNote xml:lang="en">Some of the concepts associated with this set of properties are URIs, identifiers, keys, indices, references, semes, propositions (w/o objects), codes, selections, directional, citations, pronouns.</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#denotatives">
  <rdfs:subPropertyOf rdf:resource="#representations"/>
  <core:prefLabel xml:lang="en">denotatives</core:prefLabel>
  <core:definition xml:lang="en">These ae icons or symbols that name or describe the subject.</core:definition>
  <core:scopeNote xml:lang="en">A Firstness</core:scopeNote>
  <core:scopeNote xml:lang="en">Some of the concepts associated with this set of properties are names, labels, images, descriptions, definitions, denotations, icons, designations, proper nouns.</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#descriptions">
  <rdfs:subPropertyOf rdf:resource="#denotatives"/>
  <core:prefLabel xml:lang="en">descriptions</core:prefLabel>
  <core:definition xml:lang="en">Symbol or text strings that are longer than labels and provide additional or contextual information about the object beyond simply drawing attention to it.</core:definition>
  <core:altLabel xml:lang="en">abstracts</core:altLabel>
  <core:altLabel xml:lang="en">definitions</core:altLabel>
  <core:scopeNote xml:lang="en">A Thirdness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#hasCharacteristic">
  <rdfs:subPropertyOf rdf:resource="#relateds"/>
  <core:prefLabel xml:lang="en">has characteristic</core:prefLabel>
  <core:definition xml:lang="en">The property kko:hasCharacteristic is used to assert the relation between a Reference Concept and external properties that may be used in external ontologies to characterize, describe, or provide attributes for data records associated with that concept. It is via this property or its inverse, kko:isCharacteristicOf, that external data characterizations may be incorporated and modeled within a domain ontology based on the KKO vocabulary.</core:definition>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#relateds">
  <rdfs:subPropertyOf rdf:resource="#associatives"/>
  <core:prefLabel xml:lang="en">relateds</core:prefLabel>
  <core:definition xml:lang="en">An indicator of some nature to other objects similar or related to the given object; the criteria and degree or strength of relationship between the items are indeterminant.</core:definition>
  <core:altLabel xml:lang="en">see also</core:altLabel>
  <core:scopeNote xml:lang="en">A Secondness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#identifiers">
  <rdfs:subPropertyOf rdf:resource="#indexes"/>
  <core:prefLabel xml:lang="en">identifiers</core:prefLabel>
  <core:definition xml:lang="en">Generally (unique) symbols or strings that provide a key to the given subject, often within some conventional scheme for generating and recognizing the token assigned.</core:definition>
  <core:altLabel xml:lang="en">ID</core:altLabel>
  <core:scopeNote xml:lang="en">A Secondness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#isCharacteristicOf">
  <rdfs:subPropertyOf rdf:resource="#relateds"/>
  <core:prefLabel xml:lang="en">is characteristic of</core:prefLabel>
  <core:definition xml:lang="en">The property kko:isCharacteristicOf is used to assert the relation between a property and a Reference Concept (or its punned individual) to which it applies. Such properties may be used in external ontologies to characterize, describe, or provide attributes for data records associated with that concept. It is via this property or its inverse, kko:hasCharacteristic, that external data characterizations may be incorporated and modeled within a domain ontology based on the KKO vocabulary.</core:definition>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#labels">
  <rdfs:subPropertyOf rdf:resource="#denotatives"/>
  <core:prefLabel xml:lang="en">labels</core:prefLabel>
  <core:definition xml:lang="en">Symbolic text strings that help to name or draw attention to a particular object.</core:definition>
  <core:altLabel xml:lang="en">names</core:altLabel>
  <core:scopeNote xml:lang="en">A Secondness</core:scopeNote>
  <core:scopeNote xml:lang="en">Labels such as names, proper names, titles, aliases, synonyms, misspellings, honorifics and such that draw attention to a particular object are all part of the property category.</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#lists">
  <rdfs:subPropertyOf rdf:resource="#associatives"/>
  <core:prefLabel xml:lang="en">lists</core:prefLabel>
  <core:definition xml:lang="en">An aggregation, either ordered or unordered, of objects similar to one another with respect to given characters or types.</core:definition>
  <core:scopeNote xml:lang="en">A Firstness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#media">
  <rdfs:subPropertyOf rdf:resource="#denotatives"/>
  <core:prefLabel xml:lang="en">media</core:prefLabel>
  <core:definition xml:lang="en">Iconic images or sounds that invoke the identification with a given object or representation. Media in this sense draws attention to the object.</core:definition>
  <core:altLabel xml:lang="en">images</core:altLabel>
  <core:scopeNote xml:lang="en">A Firstness</core:scopeNote>
</owl:AnnotationProperty>

<owl:AnnotationProperty rdf:about="#pointers">
  <rdfs:subPropertyOf rdf:resource="#indexes"/>
  <core:prefLabel xml:lang="en">pointers</core:prefLabel>
  <core:definition xml:lang="en">Physical or symbolic indicators of a given thing and which draw attention to it.</core:definition>
  <core:altLabel xml:lang="en">indicators</core:altLabel>
  <core:scopeNote xml:lang="en">A Firstness</core:scopeNote>
</owl:AnnotationProperty>

